    {
      "id": 1,
      "title": "BCB SGS",
      "type": "stat",
      "gridPos": { "h": 4, "w": 8, "x": 0, "y": 1 },
      "datasource": { "type": "postgres", "uid": "timescaledb" },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "postgres", "uid": "timescaledb" },
          "rawSql": "SELECT status, duration_seconds, started_at\nFROM pipeline_runs\nWHERE step_name = 'bcb_sgs'\nORDER BY started_at DESC LIMIT 1",
          "format": "table",
          "editorMode": "code"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "SUCCESS": { "text": "SUCCESS", "color": "green", "index": 0 } } },
            { "type": "value", "options": { "FAILED": { "text": "FAILED", "color": "red", "index": 1 } } },
            { "type": "value", "options": { "RUNNING": { "text": "RUNNING", "color": "yellow", "index": 2 } } }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null }
            ]
          },
          "color": { "mode": "thresholds" }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": "/^status$/"
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      }
    },
