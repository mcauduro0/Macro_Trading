    {
      "id": 8,
      "title": "Recent Pipeline Errors",
      "type": "table",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 19 },
      "datasource": { "type": "postgres", "uid": "timescaledb" },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "postgres", "uid": "timescaledb" },
          "rawSql": "SELECT started_at AS \"Time\", step_name AS \"Step\", error_message AS \"Error\"\nFROM pipeline_runs\nWHERE status = 'FAILED' AND $__timeFilter(started_at)\nORDER BY started_at DESC LIMIT 20",
          "format": "table",
          "editorMode": "code"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "cellOptions": { "type": "auto" },
            "inspect": false
          },
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "red", "value": null }]
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Time" },
            "properties": [{ "id": "custom.width", "value": 200 }]
          },
          {
            "matcher": { "id": "byName", "options": "Step" },
            "properties": [{ "id": "custom.width", "value": 200 }]
          }
        ]
      },
